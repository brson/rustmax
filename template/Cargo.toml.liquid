[workspace.package]
description = "todo"
version = "0.1.0"
license = "Apache-2.0 OR MIT"
authors = ["Brian Anderson <andersrb@gmail.com>"]
documentation = "todo"
edition = "2021"

[workspace]
resolver = "2"

members = [
  "crates/*",
]

default-members = [
  "crates/{{project-name}}-cli",
]

[workspace.dependencies]
rmx.path = "../rmx/crates/rmx"
rmx.version = "0.2.0"
rmx.features = [
  "rmx-profile-std",
  "rmx-feature-derive",
]
